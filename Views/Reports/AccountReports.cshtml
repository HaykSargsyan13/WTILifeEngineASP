@{
    ViewData["Title"] = "Account Reports";
    Layout = "_ReportsLayout";
}
<body>
    <h2>@ViewData["Title"]</h2>

    <header>
        Elapsed time   @ViewBag.Timer
    </header>
    <header>
        Total Count   @ViewBag.Count
    </header>
    <form asp-action="AccountReports" asp-controller="Reports">
        <div>
            <div class="form-group">
                <label>AccountNo</label>
                <input id="AccountNo" name="AccountNo" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Find" class="btn btn-default" />
            </div>
        </div>
    </form>
    <div class="right">
        <a asp-controller="Home" asp-action="Index">Back to home page</a>
    </div>
</body>