@model System.TimeSpan;
    @{ 
        Layout = "_BaseReportLayout";
    }
<form method="post" enctype="multipart/form-data" asp-controller="Test" asp-action="UploadFile">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="file" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
@if(@Model > TimeSpan.Zero)
{ 
<div class="right">TimeSpan = @Model</div>
}